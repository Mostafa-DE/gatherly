{
  "name": "gatherly",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.27.0",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "lint": "tsc --noEmit",
    "preview": "vite preview",
    "start": "node .output/server/index.mjs",
    "test": "vitest run",
    "db:up": "docker compose up -d db",
    "db:stop": "docker compose stop db",
    "db:reset": "docker compose down && docker volume rm gatherly_postgres_data 2>/dev/null; docker compose up -d db --wait && pnpm db:migrate && pnpm db:seed:interests",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "drizzle-kit migrate",
    "db:seed:demo-user": "pnpm exec tsx --env-file=.env ./scripts/create-demo-user.ts",
    "db:seed:interests": "pnpm exec tsx --env-file=.env ./scripts/seed-interests.ts",
    "db:seed:test": "pnpm exec tsx --env-file=.env ./scripts/seed-test-data.ts",
    "db:seed:test:clean": "pnpm exec tsx --env-file=.env ./scripts/seed-test-data.ts --clean",
    "auth:generate": "npx @better-auth/cli generate --output ./src/db/auth-schema.ts",
    "ollama:up": "docker compose up -d ollama && echo 'Waiting for Ollama...' && sleep 5 && docker exec gatherly-ollama ollama pull mistral:7b",
    "ollama:stop": "docker compose stop ollama",
    "railway:deploy:ollama": "sh ./scripts/railway-deploy.sh ollama",
    "railway:deploy:web": "sh ./scripts/railway-deploy.sh web"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^3.3.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-devtools": "^0.7.0",
    "@tanstack/react-query": "^5.90.19",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-router-devtools": "^1.132.0",
    "@tanstack/react-router-ssr-query": "^1.131.7",
    "@tanstack/react-start": "^1.132.0",
    "@tanstack/router-plugin": "^1.132.0",
    "@trpc/client": "^11.8.1",
    "@trpc/react-query": "^11.8.1",
    "@trpc/server": "^11.8.1",
    "better-auth": "^1.4.17",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "consola": "^3.4.2",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "jotai": "^2.16.2",
    "lucide-react": "^0.561.0",
    "next-themes": "^0.4.6",
    "nitro": "npm:nitro-nightly@latest",
    "pg": "^8.17.2",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.0",
    "react-day-picker": "^9.13.0",
    "react-dom": "^19.2.0",
    "recharts": "2.15.4",
    "sonner": "^2.0.7",
    "superjson": "^2.2.6",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "vite-tsconfig-paths": "^6.0.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@tanstack/devtools-vite": "^0.3.11",
    "@testcontainers/postgresql": "^11.11.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "drizzle-kit": "^0.31.8",
    "jsdom": "^27.0.0",
    "testcontainers": "^11.11.0",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vitest": "^3.0.5",
    "web-vitals": "^5.1.0"
  }
}
